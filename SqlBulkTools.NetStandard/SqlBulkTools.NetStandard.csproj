<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <LangVersion>7.2</LangVersion>
  </PropertyGroup>
  <PropertyGroup Condition=" '$(Configuration)' == 'Release' ">
    <DebugSymbols>true</DebugSymbols>
    <DebugType>full</DebugType>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="System.ComponentModel.Annotations" Version="4.5.0" />
    <PackageReference Include="System.Data.SqlClient" Version="4.5.1" />
  </ItemGroup>

  <PropertyGroup>
    <GenerateAssemblyInfo>false</GenerateAssemblyInfo>
    <Authors>liningit, tiagorosendo, zek99, gtaylor44, ersensurekler</Authors>
    <RepositoryUrl>https://github.com/ersensurekler/SqlBulkTools</RepositoryUrl>
    <PackageProjectUrl>https://github.com/ersensurekler/SqlBulkTools</PackageProjectUrl>
    <PackageLicenseUrl>https://github.com/ersensurekler/SqlBulkTools/blob/master/LICENSE</PackageLicenseUrl>
    <PackageTags>SQL BULK BULKINSERT BULKUPDATE BULKINSERTORUPDATE</PackageTags>
    <GeneratePackageOnBuild>false</GeneratePackageOnBuild>
  </PropertyGroup>
</Project>